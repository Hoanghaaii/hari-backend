{
  "name": "hari-backend",
  "version": "1.0.0",
  "description": "E-commerce microservices system with NestJS and Kafka",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
